        "#ADVANCED CONTENT LABELING & PROMPT DEVELOPMENT\n",
        "#TRY No.1\n",
        "\n",
        "import pandas as pd\n",
        "\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/AI in Research Course/propaganda_data_clean.xlsx - check.csv.csv\")\n",
        "\n",
        "\n",
        "def label_row_gpt(question, text):\n",
        "    question = str(question) if question else \"\"\n",
        "    text = str(text) if text else \"\"\n",
        "    text = text[:3000]\n",
        "\n",
        "    prompt = (\"You are analyzing whether an article supports, rejects, or ambiguously presents a propaganda claim described in the question below.\\n\\n\"\n",
        "        \"Assign:\\n\"\n",
        "        \"- 1 if the article clearly supports the claim without critique.\\n\"\n",
        "        \"- 0 if the article clearly rejects, debunks, or undermines the claim.\\n\"\n",
        "        \"- 2 if the article presents mixed views, is neutral, or does not address the claim.\\n\\n\"\n",
        "        \"Ignore irrelevant or noisy content and focus only on the core topic listed in the questioin.\\n\\n\"\n",
        "        f\"QUESTION: {question}\\n\\n\"\n",
        "        f\"ARTICLE:\\n{text}\\n\\n\"\n",
        "        \"Answer with only: 0, 1, or 2.\"\n",
        "    )\n",
        "\n",
        "    try:\n",
        "        response = client.chat.completions.create(\n",
        "            model=\"gpt-4\",\n",
        "            messages=[\n",
        "                {\"role\": \"system\", \"content\": \"You are a precise labeling assistant.\"},\n",
        "                {\"role\": \"user\", \"content\": prompt}\n",
        "            ],\n",
        "            max_tokens=5,\n",
        "            temperature=0\n",
        "        )\n",
        "        answer = response.choices[0].message.content.strip()\n",
        "        return int(answer) if answer in {\"0\", \"1\", \"2\"} else 2\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"⚠️ Error labeling row: {e}\")\n",
        "        return 2\n",
        "\n",
        "\n",
        "\n",
        "df[\"gpt_label\"] = df.apply(\n",
        "    lambda row: label_row_gpt(row[\"question\"], row[\"Simplified Text\"]),\n",
        "    axis=1\n",
        ")\n",
        "\n",
        "from IPython.display import display\n",
