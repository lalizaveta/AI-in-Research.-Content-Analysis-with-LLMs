        "#Source classification task\n",
        "\n",
        "def classify_article_source(text):\n",
        "    text = str(text) if text else \"\"\n",
        "    text = text[:3000]\n",
        "\n",
        "    prompt = (\n",
        "        \"You are a classification assistant. Your task is to identify the main source or origin of the information in the article below, and classify it into one of the following categories:\\n\\n\"\n",
        "        \"- official site (e.g. government websites, ministries, embassies)\\n\"\n",
        "        \"- media outlet (e.g. news websites, TV channels, newspapers)\\n\"\n",
        "        \"- blog or opinion (e.g. personal websites, commentary blogs)\\n\"\n",
        "        \"- NGO (e.g. non-governmental humanitarian or advocacy groups)\\n\"\n",
        "        \"- academic (e.g. universities, research institutions)\\n\"\n",
        "        \"- unknown (if the source is not identifiable)\\n\\n\"\n",
        "        \"Focus on the content, not just the domain name or format. Do not return any explanation.\\n\\n\"\n",
        "        f\"ARTICLE:\\n{text}\\n\\n\"\n",
        "        \"Respond with only one category from the list above.\"\n",
        "    )\n",
        "\n",
        "    try:\n",
        "        response = client.chat.completions.create(\n",
        "            model=\"gpt-4\",\n",
        "            messages=[\n",
        "                {\"role\": \"system\", \"content\": \"You are a reliable content classification assistant.\"},\n",
        "                {\"role\": \"user\", \"content\": prompt}\n",
        "            ],\n",
        "            max_tokens=10,\n",
        "            temperature=0\n",
        "        )\n",
        "        return response.choices[0].message.content.strip().lower()\n",
        "    except Exception as e:\n",
        "        print(f\"⚠️ Error: {e}\")\n",
        "        return \"unknown\"\n",
        "\n",
        "# Make a copy of the first 10 rows\n",
        "subset = df.head(10).copy()\n",
        "\n",
        "# Apply the classification function\n",
        "subset[\"gpt_source\"] = subset[\"Simplified Text\"].apply(classify_article_source)\n",
        "\n",
        "# Display the result\n",
        "from IPython.display import display\n",
        "display(subset[[\"Simplified Text\", \"gpt_source\"]])\n",
        "\n",
        "# Save to file\n",
        "subset.to_csv(\"/content/drive/MyDrive/gpt_source_sample.csv\", index=False)\n"
